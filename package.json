    "@types/stripe": "^8.0.416",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "config": "^3.3.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.1",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
